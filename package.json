    "@discordjs/voice": "^0.19.0",
    "@distube/ytdl-core": "^4.16.12",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/generative-ai": "^0.24.1",
    "@snazzah/davey": "^0.1.7",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "fluent-ffmpeg": "^2.1.3",
    "libsodium-wrappers": "^0.7.15",
    "play-dl": "^1.9.7",
    "sodium-native": "^5.0.9",
    "youtube-dl-exec": "^3.0.26",
    "youtube-sr": "^4.3.12"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
